{
  "name": "quantic-ui",
  "version": "0.1.12",
  "description": "quantic design system",
  "main": "dist/index.js",
  "repository": "https://github.com/gabrielgrs/quantic-ui",
  "author": "Gabriel Ribeiro <grxgabriel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build": "BABEL_ENV=production babel src -d dist",
    "prepublish": "yarn build",
    "remove:dist": "rm -rf dist",
    "postpublish": "yarn remove:dist"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "rm -rf dist"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "babel-preset-react-app": "7.0.0",
    "polished": "^3.4.2",
    "react": "^16.12.0",
    "react-datepicker": "^2.11.0",
    "react-dom": "^16.12.0",
    "react-input-mask": "^2.0.4",
    "react-responsive-carousel": "^3.1.51",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "react-select": "^3.0.8",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-docs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addon-storysource": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.9",
    "auto": "^9.4.1",
    "husky": "^4.2.1"
  }
}
